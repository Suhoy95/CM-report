{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "*.aux": true
  },
  "editor.rulers": [
    60, 80, 100
  ],
  "editor.renderWhitespace": "boundary",
  "files.trimTrailingWhitespace": true,
  "python.pythonPath": "/usr/bin/python3",
  "cSpell.words": [
    "Enter",
    "Image",
    "Local",
    "abstract",
    "alpha",
    "appendix",
    "backmatter",
    "cases",
    "cite",
    "clearpage",
    "floatstyle",
    "include",
    "integral",
    "latex",
    "listings",
    "macros",
    "mainmatter",
    "matplotlib",
    "mode",
    "newtheorem",
    "numpy",
    "preamble",
    "russian",
    "verbatim"
  ]
}